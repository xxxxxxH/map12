<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <jahirfiquitiva.libs.fabsmenu.FABsMenuLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        fab:fabs_menu_clickableOverlay="true"
        fab:fabs_menu_overlayColor="#4d000000"
        tools:layout_behavior="@string/fabs_menu_layout_behavior">

        <jahirfiquitiva.libs.fabsmenu.FABsMenu
            android:id="@+id/fabs_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:clipChildren="false"
            fab:fab_expandDirection="up"
            fab:fab_labelsPosition="left"
            fab:fab_menuMargins="16dp"
            fab:fab_moreButtonPlusIcon="@mipmap/add"
            fab:fab_moreButtonBackgroundColor="@color/yellowcolor"
            fab:fab_moreButtonRippleColor="#4d000000"
            fab:fab_moreButtonSize="normal">

            <!-- This is the item that appears at the top of the menu -->
            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/mapOutdoor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/colorAccent"
                fab:fabSize="mini"
                fab:fab_title="Outdoor"
                fab:fab_title_backgroundColor="@color/colorAccent"
                fab:fab_title_textColor="@android:color/white"
                fab:rippleColor="@color/colorAccent" />

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/mapSatellite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/colorAccent"
                fab:fabSize="mini"
                fab:fab_enableTitleClick="true"
                fab:fab_title="Satellite"
                fab:fab_title_backgroundColor="@color/colorAccent"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/colorAccent"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/mapTraffic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/colorAccent"
                fab:fabSize="mini"
                fab:fab_enableTitleClick="true"
                fab:fab_title="Traffic"
                fab:fab_title_backgroundColor="@color/colorAccent"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/colorAccent"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/mapLight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/colorAccent"
                fab:fabSize="mini"
                fab:fab_title="Light"
                fab:fab_title_backgroundColor="@color/colorAccent"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/colorAccent"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/streetView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/orange"
                fab:fabSize="normal"
                fab:fab_title="Street View"
                fab:fab_title_backgroundColor="@color/orange"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/orange"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/router"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/pink"
                fab:fabSize="normal"
                fab:fab_title="Router"
                fab:fab_title_backgroundColor="@color/pink"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/pink"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/near"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/green"
                fab:fabSize="normal"
                fab:fab_title="Near by"
                fab:fab_title_backgroundColor="@color/green"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/green"/>

            <jahirfiquitiva.libs.fabsmenu.TitleFAB
                android:id="@+id/inter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                fab:backgroundTint="@color/red"
                fab:fabSize="normal"
                fab:fab_title="Interactive"
                fab:fab_title_backgroundColor="@color/red"
                fab:fab_title_textColor="@color/white"
                fab:rippleColor="@color/red"/>

        </jahirfiquitiva.libs.fabsmenu.FABsMenu>
    </jahirfiquitiva.libs.fabsmenu.FABsMenuLayout>
</RelativeLayout>